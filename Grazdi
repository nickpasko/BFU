//c++. В первой строке - предложение. Следом вводится символ. Программа должна вывести кол-во этих символов в предложении.



#include <cstdlib>
#include <iostream>
#include <string>

using namespace std;

int main(int argc, char *argv[])
{
    string s;
    char c;
    int k=0;
    getline(cin, s);  
    cin >> c;
    for (int i=0; i<=s.size(); i++)
        if (s[i]==c)
           k++;
    cout << k;
    system("PAUSE");
    return EXIT_SUCCESS;
}
